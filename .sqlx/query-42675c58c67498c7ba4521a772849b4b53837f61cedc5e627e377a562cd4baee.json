  "query": "SELECT amps, volts, watts, created_at, user_agent, client_ip, energy_log.token as token, u.location as location \n        FROM energy_log\n        INNER JOIN tokens t\n        ON t.token = energy_log.token\n        INNER JOIN users u\n        ON u.id = t.user_id\n        WHERE energy_log.token = ?\n        ORDER BY created_at DESC\n        LIMIT ?\n        OFFSET ?",
